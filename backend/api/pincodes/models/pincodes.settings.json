{
  "kind": "collectionType",
  "collectionName": "pincodes",
  "info": {
    "name": "Pincodes"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "codes": {
      "type": "text",
      "regex": "^[-+]?(?:[0-9]+,)*[0-9]+(?:\\.[0-9]+)?$",
      "required": true
    }
  }
}
